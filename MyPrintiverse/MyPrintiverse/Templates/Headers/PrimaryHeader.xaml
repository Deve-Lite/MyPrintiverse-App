<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MyPrintiverse.Templates.Headers.PrimaryHeader"
             x:Name="this">
    
    <VerticalStackLayout BindingContext="{x:Reference this}">
        <Label Style="{StaticResource PrimaryHeader_Title}"
               Text="{Binding Title}"/>

        <Border Style="{StaticResource PrimaryHeader_Border}"/>
    </VerticalStackLayout>
    
</ContentView>
