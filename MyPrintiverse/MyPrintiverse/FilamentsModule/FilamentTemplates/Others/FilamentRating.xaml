<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MyPrintiverse.FilamentsModule.FilamentTemplates.Others.FilamentRating"
             x:Name="this">
    
    <Grid BindingContext="{x:Reference this}" 
          RowDefinitions="*,*,*,*,*">
        <Image x:Name="TopImage" Grid.Row="0"/>
        <Image x:Name="TopMiddleImage" Grid.Row="1"/>
        <Image x:Name="MiddleImage" Grid.Row="2"/>
        <Image x:Name="BottomMiddleImage" Grid.Row="3"/>
        <Image x:Name="BottomImage" Grid.Row="4"/>
    </Grid>
</ContentView>
